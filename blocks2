# default values for blocks
default height:20 

# block definitions
bdef symbol:a width:10 fill:color(RGB(70,10,10)) stroke:color(black)
bdef symbol:b width:10 fill:color(RGB(70,10,10)) 
bdef symbol:c width:10 fill:color(red) stroke:color(blue)
bdef symbol:n width:10 fill:image(filename.png) stroke:color(black)

# spacers definitions
sdef symbol:* width:20
sdef symbol:^ width:10